{
  "name": "mobx-container",
  "version": "0.0.3",
  "description": "simple mobx container for react component",
  "main": "src/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run build",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --plugins add-module-exports src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ali322/mobx-container.git"
  },
  "keywords": [
    "mobx",
    "react"
  ],
  "author": "alichen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ali322/mobx-container/issues"
  },
  "homepage": "https://github.com/ali322/mobx-container#readme",
  "peerDependencies": {
    "mobx": "^3.1.16"
  },
  "dependencies": {
    "mobx-react": "^4.2.1",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "rimraf": "^2.6.1"
  }
}
